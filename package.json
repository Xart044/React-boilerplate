{
  "name": "react-boilerplate",
  "version": "1.1.0",
  "description": "react boilerplate with cutting-edge features",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "dev": "NODE_ENV=development node server.js",
    "prod": "NODE_ENV=production node server.js",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --colors",
    "postinstall": "NODE_ENV=production webpack -p --progress --colors",
    "clean": "rimraf ./dist/*"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "js",
    "frontend"
  ],
  "author": "Artem Vergun",
  "license": "ISC",
  "engines": {
    "node": "8.4.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.7",
    "directory-named-webpack-plugin": "^2.2.3",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.6.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.6.2",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
